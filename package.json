{
  "name": "rent-manager",
  "version": "2.6.6",
  "description": "rent-manager for ying",
  "author": "rhinel <rhinelkc@hotmail.com>",
  "private": true,
  "scripts": {
    "git-init": "git submodule update --init --recursive",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "serve": "cd ./server && supervisor --inspect index.js",
    "lint": "eslint --ext .js,.vue src server",
    "lint:css": "stylelint **/*.{vue,css,less} --config ./.stylelintrc-feat.js",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --config ./.stylelintrc-feat.js",
      "git add"
    ],
    "*.scss": [
      "stylelint --config ./.stylelintrc-feat.js",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-chinese"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "got": "^9.6.0",
    "log4js": "^4.3.2",
    "md5": "^2.2.1",
    "mongoose": "^5.6.1",
    "redis": "^2.8.0",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@tweenjs/tween.js": "^17.4.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-register": "^6.26.0",
    "cache-loader": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "commitizen": "^3.1.1",
    "compression-webpack-plugin": "^3.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "cz-conventional-changelog-chinese": "^2.3.0",
    "element-ui": "^2.9.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
